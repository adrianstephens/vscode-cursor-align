{
  "name": "cursor-align",
  "displayName": "Cursor Align",
  "description": "Aligns all cursors using spaces.",
  "icon": "img/icon.png",
  "version": "1.0.4",
  "publisher": "yo1dog",
  "engines": {
    "vscode": "^1.15.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:yo1dog.cursor-align.alignCursors"
  ],
  "main": "./extension.js",
  "browser": "./extension.js",
  "extensionKind": ["ui", "workspace"],
  "contributes": {
    "commands": [
      {
        "command": "yo1dog.cursor-align.alignCursors",
        "title": "Align Cursors"
      }
    ],
    "keybindings": [
      {
        "command": "yo1dog.cursor-align.alignCursors",
        "key": "alt+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "eslint": "^8.3.0"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/yo1dog/vscode-cursor-align/issues",
    "email": "mike@yo1.dog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yo1dog/vscode-cursor-align.git"
  },
  "homepage": "https://github.com/yo1dog/vscode-cursor-align"
}
